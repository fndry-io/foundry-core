{
    "name": "foundry/core",
    "description": "Foundry Framework core package for managing central contracts across the Foundry Framework.",
    "keywords": [
        "foundry",
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "foundry-core",
    "require": {
        "php": "^7.3|^8.0",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^8.12",
        "laravel/tinker": "^2.5",
        "endyjasmi/cuid": "^2.0",
        "erusev/parsedown": "^1.7",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "Foundry\\Core\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Foundry\\Core\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Foundry\\Core\\CoreServiceProvider"
            ],
            "aliases": {
            }
        }
    },
    "scripts": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
